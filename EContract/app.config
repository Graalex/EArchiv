<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<configSections>
		<section name="modules" type="Microsoft.Practices.Prism.Modularity.ModulesConfigurationSection, Microsoft.Practices.Prism"/>
	</configSections>

	<startup>
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0,Profile=Client"/>
	</startup>

	<connectionStrings>
		<add name="Security" connectionString="Data Source=DATA-SERV;Initial Catalog=Security;User ID=ARCHIV_APP;Password=ARCHIV_APP"/>
	</connectionStrings>

	<modules>
		<module assemblyFile="mariupolgaz.earchiv.security" moduleType="Mariupolgaz.EArchiv.Security.SecurityModule, Mariupolgaz.EArchiv.Security, Version=1.0.0.1, Culture=neutral, PublicKeyToken=null"
            moduleName="SecurityService" startupLoaded="true"/>
		<module assemblyFile="mariupolgaz.econtract.finder" moduleType="Mariupolgaz.EContract.Finder.ContractFinderModule, Mariupolgaz.EContract.Finder, Version=1.0.0.1, Culture=neutral, PublicKeyToken=null"
            moduleName="ContractFinder" startupLoaded="true"/>
	</modules>
</configuration>